{
  "version": 2,
  "name": "duplicate-detection-system",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "functions": {
    "api/health.js": {
      "maxDuration": 25,
      "memory": 1024
    },
    "api/process-duplicates.js": {
      "maxDuration": 25,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}